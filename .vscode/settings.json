// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/.vscode": true,
        "**/.sass-cache":true,
        "**/_site":true
    },
    "markdown.styles": [
        "**/_site"
    ],
    "spellright.language": "en",
    "spellright.documentTypes": [
        "markdown",
        "latex",
        "plaintext",
        "scss"
    ],
    "files.eol": "\n",
    "cSpell.enabledLanguageIds": [
        "csharp",
        "css",
        "git-commit",
        "html",
        "javascript",
        "javascriptreact",
        "jekyll",
        "json",
        "jsonc",
        "less",
        "markdown",
        "plaintext",
        "scss",
        "sql",
        "text",
        "typescript",
        "typescriptreact",
        "yaml",
        "yml"
    ],
    "cSpell.language": "en,en-US",
    "workbench.colorCustomizations": {
        // https://paletton.com/#uid=73-0u0kfsib8-rHcFmBk5eOoSba
        "activityBar.background": "#b4a36d",
        "titleBar.activeBackground": "#ddcfa2",
        "titleBar.activeForeground": "#10253a"
    },
    "cSpell.words": [
        "mandalorian"
    ]
}